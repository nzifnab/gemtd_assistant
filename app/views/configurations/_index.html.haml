#load-window.modal.hide.fade{role: 'dialog', tabindex: '-1'}
  .modal-header
    %button.close{type: 'button', data: {dismiss: 'modal'}}
      %i.icon-remove
    %h3 Load Configuration
  .modal-body
    %form.form-inline.js-saved-configurations
      .btn-group{data: {toggle: 'buttons-radio'}}
        %ul.unstyled
  .modal-footer
    %button.btn{data: {dismiss: "modal"}} Close
    %button.btn.btn-primary Load Selected
